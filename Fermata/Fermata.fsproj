<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageId>Fermata</PackageId>
    <Version>0.0.1</Version>
    <Authors>taidalog</Authors>
    <Company />
    <Description>F# miscellaneous library</Description>
    <PackageProjectUrl>https://github.com/taidalog/Fermata</PackageProjectUrl>
    <PackageTags>F# FSharp miscellaneous library functional programming</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Copyright>Copyright (c) 2022-2023 taidalog</Copyright>
    <RepositoryUrl>https://github.com/taidalog/Fermata.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Array.fs" />
    <Compile Include="List.fs" />
    <Compile Include="Seq.fs" />
    <Compile Include="Bool.fs" />
    <Compile Include="RadixConversion.fs" />
    <Compile Include="Regex.fs" />
    <Compile Include="Result.fs" />
    <Compile Include="String.fs" />
    <Compile Include="Tuple.fs" />
    <None Include="..\README.md" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
